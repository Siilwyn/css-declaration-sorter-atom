{
  "name": "css-declaration-sorter",
  "version": "1.1.2",
  "description": "Sorts CSS declarations fast and automatically in a certain order.",
  "main": "./lib/css-declaration-sorter",
  "files": [
    "keymaps",
    "lib",
    "menus"
  ],
  "scripts": {
    "start": "atom .",
    "test": "atom --test ./spec/main-spec.js",
    "lint": "eslint lib/*.js spec/*.js"
  },
  "activationCommands": {
    "atom-text-editor": [
      "css-declaration-sorter:sort",
      "css-declaration-sorter:sort-alphabetically",
      "css-declaration-sorter:sort-smacss",
      "css-declaration-sorter:sort-concentric-css"
    ]
  },
  "dependencies": {
    "css-declaration-sorter": "^3.0.0",
    "postcss": "^6.0.0",
    "postcss-less": "^1.1.0",
    "postcss-scss": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^4.0.0"
  },
  "engines": {
    "atom": ">=1.5.0 <2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Siilwyn/css-declaration-sorter-atom.git"
  },
  "author": "Selwyn <talk@selwyn.cc> (https://selwyn.cc/)",
  "license": "MIT",
  "keywords": [
    "postcss",
    "css",
    "declaration",
    "sorter",
    "property",
    "order"
  ]
}
